{
  "name": "anicca",
  "type": "module",
  "version": "1.0.0",
  "description": "A declarative web application framework",
  "main": "index.js",
  "repository": "git@github.com:noamr/anicca.git",
  "author": "Noam Rosenthal <noam.j.rosenthal@gmail.com>",
  "license": "MIT",
  "scripts": {
    "grammar:formula": "nearleyc ./src/builder/formula.ne > ./src/builder/formula.ne.cjs",
    "grammar:full": "nearleyc ./src/builder/kal.ne > ./src/builder/kal.ne.cjs",
    "grammar": "yarn grammar:formula && yarn grammar:full",
    "cypress:prepare": "ts-node ./build.ts cypress/**/*.kal",
    "cypress:run": "yarn cypress:prepare && cypress run",
    "cypress:open": "cypress open",
    "build:dev": "rollup --environment BUILD:dev -c rollup.config.js",
    "build:prod": "rollup --environment BUILD:production -c rollup.config.js",
    "build:serve": "rollup --environment SERVE=1;BUILD:dev -c rollup.config.js",
    "jest": "jest",
    "lint": "tslint --project .",
    "tsc": "tsc",
    "test": "yarn lint && yarn tsc && yarn jest"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.7.5",
    "@babel/preset-typescript": "^7.7.7",
    "@babel/register": "^7.7.7",
    "@cypress/webpack-preprocessor": "^4.1.1",
    "@shieldsbetter/nearley-indentify": "^0.0.10",
    "@types/jest": "^24.9.1",
    "@types/lodash": "^4.14.149",
    "@types/lodash-es": "^4.17.3",
    "@types/nearley": "^2.11.1",
    "@types/object-hash": "^1.3.0",
    "@types/yaml": "^1.2.0",
    "cypress": "^3.8.0",
    "glob": "^7.1.6",
    "jest": "^24.9.0",
    "lodash": "^4.17.15",
    "lodash-es": "^4.17.15",
    "moo": "^0.5.1",
    "nearley": "^2.19.0",
    "object-hash": "^2.0.1",
    "rollup": "^1.27.13",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-emit-ejs": "^2.0.4",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-rust": "^1.2.0",
    "rollup-plugin-serve": "^1.0.1",
    "rollup-plugin-terser": "^5.1.3",
    "rollup-plugin-typescript": "^1.0.1",
    "rollup-plugin-web-worker-loader": "^0.8.1",
    "ts-jest": "^24.2.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.5.4",
    "tslint": "^5.20.1",
    "typescript": "^3.7.4",
    "webpack": "^4.41.4",
    "yaml": "^1.7.2",
    "yaml2json": "^1.0.2",
    "yargs": "^15.0.2"
  },
  "dependencies": {
    "@babel/core": "^7.7.7",
    "@types/chalk": "^2.2.0",
    "chalk": "^3.0.0",
    "rollup-plugin-sourcemaps": "^0.5.0"
  }
}
